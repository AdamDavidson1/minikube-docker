<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>com.coredigital.minikubedocker</string>
    <key>ProgramArguments</key>
    <array>
      <string>/opt/local/bin/minikube</string>
      <string>start</string>
      <string>-driver=hyperkit</string>
      <string>---hyperkit-vpnkit-sock=/var/run/vpnkit.socket</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
  </dict>
</plist>